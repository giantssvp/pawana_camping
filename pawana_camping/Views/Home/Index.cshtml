<script type="text/javascript">
    $(document).ready(function () {
        $('#partial_payment').change(function () {
            var part_pay = 0; 
            if($(this).prop("checked")) {
                part_pay = 1;
            }
            
            var data = JSON.stringify({
                'adult': $('#adult').val(),
                'child': $('#child').val(),
                'part_pay': part_pay
            });

            $.ajax({
                type: "POST",
                url: "/Home/calculate_amount",
                data: data,
                dataType: 'json',
                success: function (d) { 
                    var list = d.c;
                    $('#total_cost').val(list);
                },
                error: function (err) {
                    alert(err.responseText);
                },
                contentType: 'application/json'
            });
        });
    });

